{{template "base" .}}

{{define "content"}}
<div class="container bg-white mt-5">
        <div class="row">
            <div class="col">
                <h1 class="mt-3">Reservation Summary</h1>
                {{$res := index .Data "reservation"}}
                {{$res.StartDate}}
                {{$res.EndDate}}
                {{$res.CarID}}
                {{$res.FirstName}}
                {{$res.LastName}}
            </div>
        </div>
    </div>
{{end}}